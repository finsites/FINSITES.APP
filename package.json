{
	"name": "FINSITES.APP",
	"description": "test",
	"version": "0.0.1",
	"author": "Kevin Winn",
	"bugs": "https://github.com/Finsites/FINSITES.APP/issues",
	"dependencies": {
		"lwc-services": "^1.3.12"
	},
	"devDependencies": {
		"@types/express": "^4.17",
		"@types/jest": "^24",
		"husky": "^3.0.7",
		"lint-staged": "^9.4",
		"npm-run-all": "^4.1.5"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"homepage": "https://github.com/Finsites/FINSITES.APP",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"keywords": [
		"lwc"
	],
	"license": "MIT",
	"lint-staged": {
		"**/*.{css,html,js,json,md,ts,yaml,yml}": [
			"prettier --write"
		],
		"./src/**/*.ts": [
			"eslint"
		],
		"*": [
			"git add"
		]
	},
	"nodemonConfig": {
		"watch": [
			"src/server/**/*.ts",
			"scripts/express-dev.ts"
		],
		"ext": "ts",
		"ignore": [
			"src/**/*.spec.ts",
			"src/**/*.test.ts"
		],
		"exec": "ts-node ./scripts/express-dev.ts"
	},
	"repository": "Finsites/FINSITES.APP",
	"scripts": {
		"build": "lwc-services build -m production && tsc -b ./src/server",
		"build:development": "lwc-services build",
		"lint": "eslint ./src/**/*.ts",
		"prettier": "prettier --write \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
		"prettier:verify": "prettier --list-different \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
		"serve": "lwc-services serve",
		"test:unit": "lwc-services test:unit",
		"test:unit:coverage": "lwc-services test:unit --coverage",
		"test:unit:debug": "lwc-services test:unit --debug",
		"test:unit:watch": "lwc-services test:unit --watch",
		"watch": "run-p watch:client watch:server",
		"watch:client": "lwc-services watch",
		"watch:server": "nodemon"
	}
}
